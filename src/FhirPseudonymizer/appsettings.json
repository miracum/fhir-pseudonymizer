{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "System.Net.Http.HttpClient": "Warning",
      "AspNetCore.Authentication": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AnonymizationEngineConfigPath": "/etc/anonymization.yaml",
  "AnonymizationEngineConfigInline": "",
  "PseudonymizationService": "gPAS",
  "UseSystemTextJsonFhirSerializer": false,
  "ApiKey": "",
  "Tracing": {
    "ServiceName": "fhir-pseudonymizer",
    "Enabled": false,
    "Jaeger": {
      "AgentHost": "localhost",
      "AgentPort": 6831,
      "MaxPayloadSizeInBytes": 65000
    },
    "AspNetCoreInstrumentation": {
      "RecordException": "true"
    }
  },
  "EnableMetrics": true,
  "MetricsPort": 8081,
  "gPAS": {
    "Url": "",
    "RequestRetryCount": 3,
    "Auth": {
      "Basic": {
        "Username": "",
        "Password": ""
      },
      "OAuth": {
        "TokenEndpoint": "",
        "ClientId": "",
        "ClientSecret": "",
        "Scope": "",
        "Resource": ""
      }
    },
    "Cache": {
      "SizeLimit": 2048,
      "SlidingExpirationMinutes": 5,
      "AbsoluteExpirationMinutes": 30
    },
    "Version": "1.10.1"
  },
  "Vfps": {
    "Address": "",
    "UnsafeUseInsecureChannelCallCredentials": true,
    "UseTls": false
  },
  "Features": {
    "ConditionalReferencePseudonymization": false
  }
}
