{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "System.Net.Http.HttpClient": "Information",
      "Fhir.Anonymizer.Core": "Debug"
    }
  },
  "UseSystemTextJsonFhirSerializer": true,
  "AnonymizationEngineConfigPath": "anonymization.yaml",
