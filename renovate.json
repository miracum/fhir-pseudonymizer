{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":disableDependencyDashboard",
    "helpers:pinGitHubActionDigests",
    "docker:pinDigests",
    "schedule:monthly",
    "group:allNonMajor"
  ],
  "bumpVersion": "patch",
  "packageRules": [
    {
      "matchManagers": ["github-actions"],
      "groupName": "github-actions"
    }
  ]
}
